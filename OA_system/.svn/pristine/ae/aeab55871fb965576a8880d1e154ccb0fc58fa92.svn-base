<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.dao.MenuMapper">
  <resultMap id="BaseResultMap" type="com.bean.Menu">
    <id column="id" jdbcType="INTEGER" property="id" />
    <result column="url" jdbcType="VARCHAR" property="url" />
    <result column="pId" jdbcType="INTEGER" property="pid" />
    <result column="state" jdbcType="INTEGER" property="state" />
    <result column="menuname" jdbcType="VARCHAR" property="menuname" />
  </resultMap>

  <select id="findmenubyarray" parameterType="map" resultMap="BaseResultMap">
    select * from menu where id in (
    <foreach collection="ids" separator="," item="id">
      #{id}
    </foreach>
    )
  </select>

<select id="findupmenu" resultMap="BaseResultMap">
  select * from menu where pid = #{pid}
</select>

  <select id="findmenu" parameterType="map" resultMap="BaseResultMap">
    select * from menu
    <where>

      <if test="menuname1 !=null and menuname1!=''">
        menuname like '%${menuname1}%'
      </if>
    </where>

  </select>

  <sql id="Base_Column_List">
    id, url, pId, state, menuname
  </sql>
  <select id="selectByPrimaryKey" parameterType="java.lang.Integer" resultMap="BaseResultMap">
    select 
    <include refid="Base_Column_List" />
    from menu
    where id = #{id,jdbcType=INTEGER}
  </select>
  <delete id="deleteByPrimaryKey" parameterType="java.lang.Integer">
    delete from menu
    where id = #{id,jdbcType=INTEGER}
  </delete>
  <insert id="insert" parameterType="com.bean.Menu">
    insert into menu (id, url, pId, 
      state,menuname)
    values (#{id,jdbcType=INTEGER}, #{url,jdbcType=VARCHAR}, #{pid,jdbcType=INTEGER}, 
      #{state,jdbcType=INTEGER},#{menuname,jdbcType=INTEGER})
  </insert>
  <insert id="insertSelective" parameterType="com.bean.Menu">
    insert into menu
    <trim prefix="(" suffix=")" suffixOverrides=",">
      <if test="id != null">
        id,
      </if>
      <if test="url != null">
        url,
      </if>
      <if test="pid != null">
        pId,
      </if>
      <if test="state != null">
        state,
      </if>
      <if test="menuname != null">
        menuname,
      </if>
    </trim>
    <trim prefix="values (" suffix=")" suffixOverrides=",">
      <if test="id != null">
        #{id,jdbcType=INTEGER},
      </if>
      <if test="url != null">
        #{url,jdbcType=VARCHAR},
      </if>
      <if test="pid != null">
        #{pid,jdbcType=INTEGER},
      </if>
      <if test="state != null">
        #{state,jdbcType=INTEGER},
      </if>
      <if test="state != null">
        #{menuname,jdbcType=VARCHAR},
      </if>
    </trim>
  </insert>
  <update id="updateByPrimaryKeySelective" parameterType="com.bean.Menu">
    update menu
    <set>
      <if test="url != null">
        url = #{url,jdbcType=VARCHAR},
      </if>
      <if test="pid != null">
        pId = #{pid,jdbcType=INTEGER},
      </if>
      <if test="state != null">
        state = #{state,jdbcType=INTEGER},
      </if>
      <if test="menuname != null">
        menuname = #{menuname,jdbcType=VARCHAR},
      </if>
    </set>
    where id = #{id,jdbcType=INTEGER}
  </update>
  <update id="updateByPrimaryKey" parameterType="com.bean.Menu">
    update menu
    set url = #{url,jdbcType=VARCHAR},
      pId = #{pid,jdbcType=INTEGER},
      state = #{state,jdbcType=INTEGER},
      menuname = #{menuname,jdbcType=VARCHAR}
    where id = #{id,jdbcType=INTEGER}
  </update>
</mapper>